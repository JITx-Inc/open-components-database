#use-added-syntax(esir)
defpackage ocdb/tests/test-emodel-sym :
  import core
  import collections
  import esir
  import ocdb/utils/box-symbol
  import ocdb/tests/default-harness

pcb-symbol mysymbol :
  pin a at Point(-1.0, 0.0)
  pin b at Point(1.0, 0.0)
  layer("foreground") = Circle(1.0)
  layer("foreground") = Text(">VALUE", 0.7, W, loc(2.0, 0.0))
  layer("foreground") = Text(">REF", 0.7, W, loc(2.0, 2.0))

pcb-component mycomponent :
  pin a
  pin b
  mpn = "Box-13V"
  symbol = mysymbol(a => a, b => b)

pcb-module main :
  inst c1 : mycomponent
  emodel(c1) = Resistor(10.0)

pcb-design main-design :
  module = main
  rules = default-rules
  board = {default-board(4, 10.0, 10.0)}