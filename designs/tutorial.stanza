defpackage ocdb/designs/tutorial :
  import core
  import collections
  import jitx
  import jitx/commands
  import ocdb/defaults
  import ocdb/generic-components
  import ocdb/design-vars

MIN-PKG = "0603"
pcb-landpattern 
pcb-component Comp:
  pin p

pcb-module my-design :
  inst r1 : chip-resistor(1.0e3)
