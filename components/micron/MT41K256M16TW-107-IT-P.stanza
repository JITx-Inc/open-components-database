;Component designed as point-to-point (single x16 DDR3 IC), no VTT Termination
#use-added-syntax(esir)
defpackage ocdb/micron/MT41K256M16TW-107-IT-P:
  import core
  import collections
  import math
  import esir
  import esir/utils
  import esir/gen
  import ocdb/tests/default-harness

