public pcb-bundle ddr3-dat :
  port dq : pin[16] ; data bits
  port dm : pin[2]
  port dqs : diff-pair[2]

public pcb-bundle ddr3-acc :
  port a : pin[15] ;address bits
  port ba : pin[3] ;bank address
  port ck : diff-pair
  pin cke
  pin odt
  pin nCS
  pin nRAS
  pin nCAS
  pin nWE
  pin nRESET

public pcb-bundle ddr3-power :
  port vddq : pin[9]
  port vssq : pin[9]
  port vdd : pin[9]
  port vss : pin[12]
  pin vrefdq
  pin vrefca
  pin zq

;pcb-bundle ddr3 :
