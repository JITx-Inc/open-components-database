  mpn = "SN74AUC1G74DCT"
  description = "Flip Flop 1 Element D-Type 1 Bit Positive Edge 8-LSSOP, 8-MSOP (0.110in, 2.80mm Width)"
  val ps = PinSpec $ #TABLE :
    [Ref | Int ...   | Dir  ]
    [clk  | 1     | Left  ] 
    [d    | 2     | Left  ]
    [qn   | 3     | Left  ]
    [gnd  | 4     | Left  ]
