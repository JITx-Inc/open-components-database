    Point(-6.215748, -2.413), 
    Point(6.484252, -2.413), 
    Point(6.484252, 2.159)])

  layer(Silkscreen("F-SilkS", Top)) = Text(">REF", 1.27, C, loc(0.0, 2.7), "", TrueTypeFont, false, false)
  layer(Finish(Top)) = Text(">REF", 1.0, C, loc(0.0, 0.0), "", TrueTypeFont, false, false)

  layer(Silkscreen("F-SilkS", Top)) = Line(0.12, [Point(-1.27, -2.286), Point(1.404252, -2.286)])
  layer(Silkscreen("F-SilkS", Top)) = Line(0.12, [Point(-1.262748, 2.032), Point(1.404252, 2.032)])
  layer(Silkscreen("F-SilkS", Top)) = Circle(-3.421748, -1.905, 0.35921)

  layer(Finish(Top)) = Line(0.1, [Point(-4.945748, 1.905), Point(-4.945748, -2.159)])
  layer(Finish(Top)) = Line(0.1, [Point(-4.945748, 1.905), Point(5.087252, 1.905)])
