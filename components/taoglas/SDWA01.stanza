public pcb-component component :
  datasheet = "https://cdn.taoglas.com/datasheets/SDWA.01.pdf"
  mpn = "SDWA.01"
  manufacturer = "TAOGLAS"
  reference-prefix = "Y"
  description = "2.4/5-7.125GHz Wi-Fi (Including Wi-Fi 6) Ceramic SMD Antenna"
  property(self.rated-temperature) = 85.0

  pin-properties :
    [pin:Ref | pads:Ref ... | side:Dir | electrical-type:String]
    [FEED | p[2] | Down | "Input"]
    [PCB_Trace | p[1] | Down | "Passive"]

  no-connect(self.PCB_Trace)
  assign-landpattern(SDWA01-1)
  make-box-symbol()
