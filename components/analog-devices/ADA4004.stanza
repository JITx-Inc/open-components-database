    "*"
    "R3   5  99    5.0E+03"
    "R4   6  99    5.0E+03"
    "Cdm  1   2    4.0E-12"
    "Ccm1 1  50    3.0E-12"
    "Ccm2 2  50    3.0E-12"
    "Ri2  5  5a    5.5E+03"
    "Ci2  5a   6   1.9E-13"
    "IOS  1   2    35E-09"
    "* EOS  9   1    POLY(3) 73 98 229 98 12 98 60E-6  1 1 1"
    "BOS  9   1    V={60.0E-6 + 1.0 * V(73,98) + 1.0 * V(229,98) + 1.0 * V(12,98)}"
    "Q1   5   2  7  QNX"
    "Q2   6   9  8  QNX"
    "R5a   7   4   3.9E-6"
    "R6a   8   4   3.9E-6"
    "I1   4  50    500E-06"
    "Din1   2   1    DZ"
    "Din2   1   2    DZ"
    "D101 50 1 DX"
    "D102 50 2 DX"
    "D103  1 99 DX"
    "D104  2 99 DX"
    "V1  91   50   2.2"
