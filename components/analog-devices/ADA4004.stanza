    "*          	     |   inverting input"
    "*          	     |   |   positive supply"
    "*          	     |   |   |     negative supply"
    "*          	     |   |   |     |   output"
    "*             	   |   |   |     |     |"
    ".SUBCKT ADA4004   1   2   99    50    45"
    "*"
