    "GN1  2  50  15  98  1"
    "GN2  1  50  18  98  1"
    "*"
    "* GAIN STAGE & DOMINANT POLE "
    "*"
    "G1   98 30    5  6  2.917E-03"
    "R7   30 98    8.127E+05"
    "C3   30 98    1.72E-09"
    "*"
    "V4   21 98      12.99;  Voh"
    "D5   30 21     DX"
    "V3   22 98     -12.823;  Vol"
    "D6   22 30     DX"
    "*"
    "* POLE "
    "*"
    "R12  27 98     1E+01"
    "C5   27 98     1E-10	"
    "G4   98 27     30 98 1E-01"
    "*"
    "*  zero"
    "*"
    "E1   28  98     27 98  1E6"
    "R13  28 129     1E0"
    "C6   28 129     5.0E-13"
    "R14  129 98     1E-06"
