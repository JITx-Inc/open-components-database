    [mdi[0].N      | 23           | Right   ]
    [mdi[1].P      | 20           | Right   ]
    [mdi[1].N      | 19           | Right   ]
    [mdi[2].P      | 18           | Right   ]
    [mdi[2].N      | 17           | Right   ]
    [mdi[3].P      | 14           | Right   ]
    [mdi[3].N      | 13           | Right   ]
