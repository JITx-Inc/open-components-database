    [rgmii.rxd[3]  | 42           | Left    ]
    [rgmii.tx-ctrl | 2            | Left    ]
    [rgmii.tx-clk  | 47           | Left    ]
    [rgmii.txd[0]  | 44           | Left    ]
    [rgmii.txd[2]  | 45           | Left    ]
    [rgmii.txd[1]  | 48           | Left    ]
    [rgmii.txd[3]  | 1            | Left    ]
    [mdio          | 5            | Left    ]
    [mdc           | 4            | Left    ]
    [config        | 11           | Left    ]
    [nRESET        | 12           | Left    ]
    [RSET          | 25           | Left    ]
