  port led : pin[3]
  pin clk125
  pin xtal-in
  pin xtal-out
  pin mdio
  pin mdc
  pin config
  pin nRESET
  pin RSET
  pin VSS
  pin VDDO
  pin DVDD
  pin DVDD-OUT
  pin AVDDC18 
  pin AVDD18 
  pin AVDD18-OUT 
  pin AVDD33 
  pin REG-IN 
  pin REGCAP1 
  pin REGCAP2 

  manufacturer = "Marvell"
  mpn = "88E1510-A0-NNB2C000"
  description = "Ethernet ICs Single-port Gigabit Ethernet PHY with EEE; RGMII; 2.5/3.3V IO in 48-pin QFN package"
  pin-properties :
    [pin:Ref       | pads:Int ... | side:Dir]
    [rgmii.rx-ctrl | 37           | Left    ]
    [rgmii.rx-clk  | 40           | Left    ]
    [rgmii.rxd[0]  | 38           | Left    ]
    [rgmii.rxd[2]  | 39           | Left    ]
