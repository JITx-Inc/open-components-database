    gpio.gpio => mcu.PD2
  supports gpio:
    gpio.gpio => mcu.PD3
  supports gpio:
    gpio.gpio => mcu.PD4
  supports gpio:
    gpio.gpio => mcu.PD5
  supports gpio:
    gpio.gpio => mcu.PD6
  supports gpio:
    gpio.gpio => mcu.PD7
  supports gpio:
    gpio.gpio => mcu.PE6
  supports gpio:
    gpio.gpio => mcu.PF0
  supports gpio:
    gpio.gpio => mcu.PF1
  supports gpio:
