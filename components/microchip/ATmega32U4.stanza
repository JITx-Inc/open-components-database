  supports i2c:
    i2c.scl => mcu.PD0
    i2c.sda => mcu.PD1

  val spi = spi-controller()
  supports spi:
    spi.copi => mcu.PB2
    spi.cipo => mcu.PB3
    spi.sck => mcu.PB1
    spi.cs => mcu.PB0
