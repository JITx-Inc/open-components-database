    val oc  = OpenCollector(vol, 28.0e-3)
    val dio = DigitalIO(oc, vil, vih, esp32.P3V3, esp32.GND, 50.0e-9)
    for i in 0 to 2 do:
      supports i2c:
        require io:io-pin[2] from esp32
        i2c.sda => io[0].p
        i2c.scl => io[1].p
        property(io[0].p.digital-io) = dio
        property(io[1].p.digital-io) = dio

