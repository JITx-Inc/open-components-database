    [IO[2]  |      24      |  Left ]
    [IO[4]  |      26      |  Left ]
    [IO[5]  |      29      |  Left ]
    [IO[12] |      14      |  Right ]
    [IO[13] |      16      |  Right ]
    [IO[14] |      13      |  Right ]
    [IO[15] |      23      |  Right ]
    [IO[16] |      27      |  Right ]
    [IO[17] |      28      |  Right ]
    [IO[18] |      30      |  Right ]
    [IO[19] |      31      |  Right ]
    [IO[21] |      33      |  Right ]
    [IO[22] |      36      |  Right ]
    [IO[23] |      37      |  Right ]
    [IO[25] |      10      |  Right ]
    [IO[26] |      11      |  Right ]
    [IO[27] |      12      |  Right ]
    [IO[32] |      8       |  Right ]
    [IO[33] |      9       |  Right ]
    [IO[34] |      6       |  Right ]
    [IO[35] |      7       |  Right ]
    [SD[2]  |      17      |  Right ]
    [SD[3]  |      18      |  Right ]
    [CMD    |      19      |  Right ]
    [CLK    |      20      |  Right ]
    [SD[0]  |      21      |  Right ]
    [SD[1]  |      22      |  Right ]
